<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.vainolo.phd.opm.gef.editor.OPMGraphicalEditor"
            contributorClass="com.vainolo.phd.opm.gef.editor.OPMGraphicalEditorActionBarContributor"
            default="true"
            extensions="opm"
            id="com.vainolo.phd.opm.gef.editorID"
            name="OPM GEF Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="com.vainolo.phd.opm.gef.editor.OPMLaunchConfigurationDelegate"
            delegateName="com.vainolo.phd.opm.gef.editor.OPMLaunchConfigurationDelegate"
            id="com.vainolo.phd.opm.gef.editor.OPMLaunchConfigurationDelegate"
            modes="debug,run"
            name="Executable OPM">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="com.vainolo.phd.opm.gef.editor.OPMLaunchConfigurationTabGroup"
            id="com.vainolo.phd.opm.gef.editor.OPMLaunchConfigurationTabGroup"
            type="com.vainolo.phd.opm.gef.editor.OPMLaunchConfigurationDelegate">
      </launchConfigurationTabGroup>
   </extension>

</plugin>
